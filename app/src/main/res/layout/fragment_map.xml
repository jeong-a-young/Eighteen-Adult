<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".map.MapFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="76dp"
            android:layout_gravity="top"
            android:background="#f9f9f9"
            android:orientation="horizontal" >

            <ImageView
                android:id="@+id/imageView11"
                android:layout_width="28dp"
                android:layout_height="28dp"
                android:src="@drawable/icon_back_black"
                android:layout_marginLeft="31dp"
                android:layout_gravity="center" />

            <TextView
                android:id="@+id/textView27"
                android:layout_width="186dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="45dp"
                android:fontFamily="@font/noto_sans_bold"
                android:text="지도"
                android:textAlignment="center"
                android:textColor="#000000"
                android:textSize="20sp" />
        </LinearLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <com.google.android.gms.maps.MapView
                android:id="@+id/mv_map"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:background="@drawable/tab_radious_shadow" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="260dp"
                    android:layout_gravity="bottom"
                    android:background="@drawable/tab_white_radious_layout"
                    android:gravity="bottom"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="123dp"
                        android:layout_marginBottom="35dp"
                        android:gravity="bottom"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="32dp"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/textView26"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="4dp"
                                android:fontFamily="@font/noto_sans"
                                android:text="경기도 성남시 중원구 박석로"
                                android:textColor="#8F8F8F"
                                android:textSize="14sp" />

                            <TextView
                                android:id="@+id/textView25"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="17dp"
                                android:fontFamily="@font/noto_sans_bold"
                                android:text="성남시중장기청소년쉼터"
                                android:textColor="#000000"
                                android:textSize="18sp" />

                            <TextView
                                android:id="@+id/textView21"
                                android:layout_width="76dp"
                                android:layout_height="25dp"
                                android:background="@drawable/pink_radious_layout"
                                android:fontFamily="@font/noto_sans"
                                android:gravity="center"
                                android:text="여성전용"
                                android:textColor="#000000"
                                android:textSize="12sp" />
                        </LinearLayout>

                        <de.hdodenhof.circleimageview.CircleImageView
                            android:id="@+id/circleImageView1"
                            android:layout_width="68dp"
                            android:layout_height="68dp"
                            android:layout_marginLeft="83dp"
                            android:layout_marginRight="32dp"
                            android:layout_marginBottom="24dp"
                            android:src="@mipmap/ic_launcher" />

                    </LinearLayout>

                    <View
                        android:id="@+id/view"
                        android:layout_width="match_parent"
                        android:layout_height="0.5dp"
                        android:layout_marginLeft="32dp"
                        android:layout_marginRight="32dp"
                        android:background="#b7b7b7"
                        android:backgroundTint="#b7b7b7" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="32dp"
                        android:layout_marginTop="15dp"
                        android:layout_marginRight="32dp"
                        android:layout_marginBottom="32dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageView7"
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:src="@drawable/map_icon_phone" />

                        <ImageView
                            android:id="@+id/imageView9"
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_marginLeft="28dp"
                            android:src="@drawable/map_icon_bookmark_off" />

                        <Button
                            android:id="@+id/button4"
                            android:layout_width="64dp"
                            android:layout_height="31dp"
                            android:layout_gravity="center"
                            android:layout_marginLeft="73dp"
                            android:background="@drawable/green_border_radious_btn"
                            android:fontFamily="@font/noto_sans"
                            android:minWidth="0dp"
                            android:minHeight="0dp"
                            android:stateListAnimator="@null"
                            android:text="출발"
                            android:textColor="#327D52"
                            android:textSize="14sp" />

                        <Button
                            android:id="@+id/button5"
                            android:layout_width="64dp"
                            android:layout_height="31dp"
                            android:layout_gravity="center"
                            android:layout_marginLeft="18dp"
                            android:background="@drawable/green_radious_30dp_btn"
                            android:fontFamily="@font/noto_sans"
                            android:minWidth="0dp"
                            android:minHeight="0dp"
                            android:text="도착"
                            android:stateListAnimator="@null"
                            android:textColor="#FFFFFF"
                            android:textSize="14sp" />
                    </LinearLayout>
                </LinearLayout>
            </FrameLayout>

        </FrameLayout>

    </LinearLayout>

</FrameLayout>